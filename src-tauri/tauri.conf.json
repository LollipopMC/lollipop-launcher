{
  "productName": "Lollipop",
  "version": "0.1.0",
  "identifier": "dev.zeithrold.lollipop",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:2000",
    "beforeDevCommand": "yarn frontend:dev",
    "beforeBuildCommand": "yarn frontend:build"
  },
  "app": {
    "windows": [
      {
        "title": "Lollipop",
        "width": 400,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "maximizable": false

      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "default",
        "lollipop-window"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "$schema": "../node_modules/@tauri-apps/cli/schema.json"
}
